	<complexType name="BaseSearchCriteria" abstract="true">
		<annotation>
			<documentation>Base SearchCriteria for all PO entities
			</documentation>
		</annotation>
		<sequence>
			<element name="offset" type="int" minOccurs="0">
				<annotation>
					<documentation>Offset - If offset is not specified by service
						caller, then presume it to be zero
					</documentation>
				</annotation>
			</element>
			<element name="limit" type="int" minOccurs="0">
				<annotation>
					<documentation>Limit the number of records to be fetched - If limit
						not specified, then presume it to be 100.
					</documentation>
				</annotation>
			</element>
            <element name="ctepID" minOccurs="0">
                <annotation>
                    <documentation>It will search for Person's/Organization's(HCF, RO, IO) 'ctepID'
                        using "LIKE" search
                    </documentation>
                </annotation>
                <simpleType>
                    <restriction base="string">
                        <maxLength value="255" />
                        <minLength value="1" />
                    </restriction>
                </simpleType>
            </element>
            <element name="id" type="long" minOccurs="0">
                <annotation>
                    <documentation>It will search for Person's/Organization's database Id
                        using "EXACT" search
                    </documentation>
                </annotation>
            </element>
            <element name="statusCode" type="tns:EntityStatus"
                minOccurs="0">
                <annotation>
                    <documentation>It will search for Person's/Organization's Status
                        using "EXACT" search
                    </documentation>
                </annotation>
            </element>			
			<element name="line1" type="string" minOccurs="0">
				<annotation>
					<documentation>It will search for Address-'line1' using LIKE-search
					</documentation>
				</annotation>
			</element>
			<element name="line2" type="string" minOccurs="0">
				<annotation>
					<documentation>It will search for Address-'line2' using LIKE-search
					</documentation>
				</annotation>
			</element>
			<element name="city" type="string" minOccurs="0">
				<annotation>
					<documentation>It will search for Address-'city' using LIKE-search
					</documentation>
				</annotation>
			</element>
			<element name="stateOrProvince" type="string" minOccurs="0">
				<annotation>
					<documentation>It will search for Address-'stateOrProvince' using
						LIKE-search
					</documentation>
				</annotation>
			</element>
			<element name="countryCode" type="tns:CountryISO-3166-1-alpha-3-Code"
				minOccurs="0">
				<annotation>
					<documentation>It will search for Address-'country code(alpha 3)'
						using
						Exact-search
					</documentation>
				</annotation>
			</element>
			<element name="postalcode" type="string" minOccurs="0">
				<annotation>
					<documentation>It will search for Address- 'postalcode' using
						LIKE-search
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="PersonSearchCriteria">
		<annotation>
			<documentation>SearchCriteria for Person</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:BaseSearchCriteria">
				<sequence>
					<element name="firstName" minOccurs="0">
						<annotation>
							<documentation>It will search for Person's 'firstName' using
								"LIKE" search
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<maxLength value="50" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="lastName" minOccurs="0">
						<annotation>
							<documentation>It will search for Person's 'lastName' using
								"LIKE" search
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<maxLength value="50" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="email" minOccurs="0">
						<annotation>
							<documentation>It will search for Person's 'email' using
								"LIKE"
								search
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<maxLength value="254" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="affiliation" minOccurs="0">
						<annotation>
							<documentation>It will search for Person's 'Organization'
								using
								"LIKE" search
							</documentation>
						</annotation>
						<simpleType>
							<restriction base="string">
								<maxLength value="160" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>					
					<element name="hasPendingCrsRoles" type="boolean"
						minOccurs="0">
						<annotation>
							<documentation>It will search if Person has pending
								clinicalresearchstaff
								role
								using
								"EXACT" search
							</documentation>
						</annotation>
					</element>
					<element name="hasPendingHcpRoles" type="boolean"
						minOccurs="0">
						<annotation>
							<documentation>It will search if Person has pending
								healthcareprovider
								role
								using
								"EXACT" search
							</documentation>
						</annotation>
					</element>
					<element name="hasPendingOcRoles" type="boolean" minOccurs="0">
						<annotation>
							<documentation>It will search if Person has pending
								organizationalcontact
								role
								using
								"EXACT" search
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="RoleType">
		<annotation>
			<documentation>List of all RoleType(RoleGroup) that a Person can have
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="CRS" />
			<enumeration value="HCP" />
			<enumeration value="OC" />
			<enumeration value="OPI" />
		</restriction>
	</simpleType>

	<complexType name="Affiliation">
		<annotation>
			<documentation>Person's affiliated Organization
			</documentation>
		</annotation>
		<sequence>
			<element name="orgName" type="string">
				<annotation>
					<documentation>Organization Name
					</documentation>
				</annotation>
			</element>
			<element name="pending" type="boolean">
				<annotation>
					<documentation>whether Person's affiliation with the organization
						is pending
					</documentation>
				</annotation>
			</element>
			<element name="type" type="tns:RoleType" />
		</sequence>
	</complexType>

	<complexType name="BaseSearchResult" abstract="true">
		<annotation>
			<documentation>Base SearchResult for all PO entities
			</documentation>
		</annotation>
		<sequence>
			<element name="id" type="long">
				<annotation>
					<documentation>Database ID
					</documentation>
				</annotation>
			</element>
			<element name="statusCode" type="tns:EntityStatus">
				<annotation>
					<documentation>Entity status
					</documentation>
				</annotation>
			</element>
			<element name="line1" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Address-line1
					</documentation>
				</annotation>
			</element>
			<element name="line2" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Address-line2
					</documentation>
				</annotation>
			</element>
			<element name="city" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Address-City
					</documentation>
				</annotation>
			</element>
			<element name="state" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Address-State
					</documentation>
				</annotation>
			</element>
			<element name="countryCode" type="tns:CountryISO-3166-1-alpha-3-Code"
				minOccurs="0">
				<annotation>
					<documentation>Entity Address-Country Code(Alpha 3)
					</documentation>
				</annotation>
			</element>
			<element name="countryName" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Address-Country Name
					</documentation>
				</annotation>
			</element>
			<element name="postalcode" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Address-Postalcode
					</documentation>
				</annotation>
			</element>
			<element name="emailAddresses" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity Email Addresses, separated by semi-colon
					</documentation>
				</annotation>
			</element>
			<element name="phones" type="string" minOccurs="0">
				<annotation>
					<documentation>Entity phones, separated by semi-colon
					</documentation>
				</annotation>
			</element>
			<element name="duplicateOf" type="long" minOccurs="0">
				<annotation>
					<documentation>Entity is duplicate of this database id
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="PersonSearchResult">
		<annotation>
