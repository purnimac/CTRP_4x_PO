<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="gov.nih.nci.po.webservices.types"
        xmlns:tns="gov.nih.nci.po.webservices.types" elementFormDefault="qualified"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1"
        xmlns:annox="http://annox.dev.java.net"
        xmlns:ja="http://annox.dev.java.net/org.codehaus.jackson.annotate"
        xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd
                        http://www.w3.org/2001/XMLSchema http://www.w3.org/2001/XMLSchema.xsd"
        jaxb:extensionBindingPrefixes="xjc annox">

	<annotation>
		<appinfo>
			<jaxb:schemaBindings>
				<jaxb:package name="gov.nih.nci.po.webservices.types" />
			</jaxb:schemaBindings>
		</appinfo>
	</annotation>

	<simpleType name="EntityStatus">
		<annotation>
			<documentation>List of all statuses that a PO entity can be in
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="PENDING" />
			<enumeration value="ACTIVE" />
			<enumeration value="NULLIFIED" />
			<enumeration value="INACTIVE" />
		</restriction>
	</simpleType>

	<simpleType name="ContactType">
		<annotation>
			<documentation>Type of contact information
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="EMAIL" />
			<enumeration value="PHONE" />
			<enumeration value="FAX" />
			<enumeration value="TTY" />
			<enumeration value="URL" />
		</restriction>
	</simpleType>

	<complexType name="Contact">
		<annotation>
			<documentation>A piece of contact information</documentation>
		</annotation>
		<simpleContent>
			<extension base="string">
				<attribute name="type" type="tns:ContactType" use="required" />
			</extension>
		</simpleContent>
	</complexType>

	<complexType name="Address">
		<annotation>
			<documentation>An address</documentation>
		</annotation>
		<sequence>
			<element name="line1">
				<simpleType>
					<restriction base="string">
						<maxLength value="254" />
						<minLength value="1" />
					</restriction>
				</simpleType>
			</element>
			<element name="line2" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="254" />
						<minLength value="1" />
					</restriction>
				</simpleType>
			</element>
			<element name="city">
				<simpleType>
					<restriction base="string">
						<maxLength value="50" />
						<minLength value="1" />
					</restriction>
				</simpleType>
			</element>
			<element name="stateOrProvince" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="50" />
						<minLength value="1" />
					</restriction>
				</simpleType>
			</element>
			<element name="country" type="tns:CountryISO-3166-1-alpha-3-Code" />
			<element name="postalcode" minOccurs="0">
				<simpleType>
					<restriction base="string">
						<maxLength value="20" />
						<minLength value="1" />
					</restriction>
				</simpleType>
			</element>
		</sequence>
	</complexType>

	<complexType name="Entity" abstract="true">
		<annotation>
			<documentation>Base type for all PO entities</documentation>
		</annotation>
		<sequence>
			<element name="id" type="long" minOccurs="0">
				<annotation>
					<documentation>Unique numeric identifier of a PO entity
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="EntityWithStatus" abstract="true">
		<annotation>
			<documentation>Entity with a status</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:Entity">
				<sequence>
					<element name="status" type="tns:EntityStatus">
						<annotation>
							<documentation>Current status of a PO entity
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Person">
		<annotation>
			<documentation>A human being</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:EntityWithStatus">
				<sequence>
					<element name="prefix" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="10" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="firstName">
						<simpleType>
							<restriction base="string">
								<maxLength value="50" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="middleName" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="50" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="lastName">
						<simpleType>
							<restriction base="string">
								<maxLength value="50" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="suffix" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="10" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="ctepId" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="255" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="address" type="tns:Address" />
					<element name="contact" type="tns:Contact" maxOccurs="unbounded" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Role" abstract="true">
		<annotation>
			<documentation>A role that a person or an organization can play. A
				role always involves an organization, either as a role player or as
				a role scoper.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:EntityWithStatus">
				<sequence>
					<element name="organizationId" type="long" />
					<element name="address" type="tns:Address" minOccurs="0"
						maxOccurs="unbounded" />
					<element name="contact" type="tns:Contact" minOccurs="0"
						maxOccurs="unbounded" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="PersonRole" abstract="true">
		<annotation>
            <appinfo>
                <annox:annotate>                    
                     <ja:JsonTypeInfo use="CLASS" include="PROPERTY" property="@class"/>
                </annox:annotate>                
            </appinfo>
            <documentation>A role that a person plays in an organization
            </documentation>
        </annotation>
        <complexContent>
            <extension base="tns:Role">
                <sequence>
                    <element name="personId" type="long" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

	<complexType name="HealthCareProvider">
		<annotation>
			<documentation>Health Care Provider role</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:PersonRole">
				<sequence>
					<element name="license" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="255" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ClinicalResearchStaff">
		<annotation>
			<documentation>Clinical Research Staff role</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:PersonRole">
				<sequence />
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="OrganizationalContactType">
		<annotation>
			<documentation>Organizational Contact Type
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="IRB" />
			<enumeration value="Responsible Party" />
			<enumeration value="Site" />
		</restriction>
	</simpleType>

	<complexType name="OrganizationalContact">
		<annotation>
			<documentation>Organizational Contact role</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:PersonRole">
				<sequence>
					<element name="title" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="255" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="type" type="tns:OrganizationalContactType" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="Organization">
		<annotation>
			<documentation>An organization, such as a Cancer Center
			</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:EntityWithStatus">
				<sequence>
					<element name="name">
						<simpleType>
							<restriction base="string">
								<maxLength value="160" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="ctepId" minOccurs="0">
                        <simpleType>
                            <restriction base="string">
                                <maxLength value="255" />
                                <minLength value="1" />
                            </restriction>
                        </simpleType>
                    </element>
					<element name="address" type="tns:Address" />
					<element name="contact" type="tns:Contact" maxOccurs="unbounded"
						minOccurs="0" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="OrganizationRole" abstract="true">
		<annotation>
		    <appinfo>
                <annox:annotate>                    
                     <ja:JsonTypeInfo use="CLASS" include="PROPERTY" property="@class"/>
                </annox:annotate>                
            </appinfo>
			<documentation>A role that an organization plays within the cancer
				research community
			</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:Role">
				<sequence />
			</extension>
		</complexContent>
	</complexType>

	<complexType name="HealthCareFacility">
		<annotation>
			<documentation>Health Care Facility role</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:OrganizationRole">
				<sequence>
					<element name="name" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="160" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="ctepId" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="255" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ResearchOrganization">
		<annotation>
			<documentation>Research Organization role</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:OrganizationRole">
				<sequence>
					<element name="name" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="160" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="ctepId" minOccurs="0">
						<simpleType>
							<restriction base="string">
								<maxLength value="255" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="type" type="tns:ResearchOrganizationType"
						minOccurs="0" />
					<element name="fundingMechanism" type="tns:FundingMechanism"
                        minOccurs="0" />	
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="ResearchOrganizationType">
		<annotation>
			<documentation>Research Organization Type: CCR-Cancer Center, DCY-Drug Company, CLC-Clinical Center, 
			CGP-Cooperative Group, CSM-Consortia, NWK-Network, RSB-Research Base, COP-CCOP, NCP-NCP, 
			P2C-Phase II Contract, NCTN-National Clinical Trials Network
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="CCR" />
			<enumeration value="DCY" />
			<enumeration value="CLC" />
			<enumeration value="CGP" />
			<enumeration value="CSM" />
			<enumeration value="NWK" />
			<enumeration value="RSB" />
			<enumeration value="COP" />
			<enumeration value="NCP" />
			<enumeration value="P2C" />
			<enumeration value="NCTN" />
		</restriction>
	</simpleType>
	
