	<complexType name="OversightCommittee">
		<annotation>
			<documentation>Oversight Committee role</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:OrganizationRole">
				<sequence>
					<element name="type" type="tns:OversightCommitteeType" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="OversightCommitteeType">
		<annotation>
			<documentation>Oversight Committee Type
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="Institutional Review Board (IRB)" />
			<enumeration value="Ethics Committee" />
			<enumeration value="Research Ethics Board" />
		</restriction>
	</simpleType>

	<complexType name="Family">
		<annotation>
			<documentation>Organization Family, a group of related organizations
				under the same umbrella
			</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:EntityWithStatus">
				<sequence>
					<element name="name">
						<simpleType>
							<restriction base="string">
								<maxLength value="160" />
								<minLength value="1" />
							</restriction>
						</simpleType>
					</element>
					<element name="startDate" type="date" />
					<element name="p30SerialNumber" type="string" minOccurs="0"/>
					<element name="member" type="tns:FamilyMember" minOccurs="0"
						maxOccurs="unbounded" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="FamilyMember">
		<annotation>
			<documentation>Represents a member-of relationship of an organization
				to a family
			</documentation>
		</annotation>
		<sequence>
		    <element name="familyId" type="long" />
			<element name="organizationId" type="long" />
			<element name="type" type="tns:FamilyMemberType" />
			<element name="startDate" type="date" />
			<element name="endDate" type="date" minOccurs="0" />
		</sequence>
	</complexType>

	<simpleType name="FamilyMemberType">
		<annotation>
			<documentation>Family Member Type
			</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="ORGANIZATIONAL" />
			<enumeration value="CONTRACTUAL" />
			<enumeration value="AFFILIATION" />
		</restriction>
	</simpleType>

	<complexType name="FamilyMemberRelationship">
		<annotation>
			<documentation>Represents a hierarchical relationship between any two
				members of a family. "organizationId" is a
				PEER|PARENT|CHILD|DIVISION|SUBDIVISION|DEPARTMENT|SUBDEPARTMENT of
				"relatedToOrganizationId"
			</documentation>
		</annotation>
		<complexContent>
			<extension base="tns:Entity">
				<sequence>
					<element name="familyId" type="long">
						<annotation>
							<documentation>Family ID</documentation>
						</annotation>
					</element>
					<element name="organizationId" type="long">
						<annotation>
							<documentation>The 'From' end of the relationship.
							</documentation>
						</annotation>
					</element>
					<element name="relatedToOrganizationId" type="long">
						<annotation>
							<documentation>The 'To' end of the relationship.</documentation>
						</annotation>
					</element>
					<element name="type" type="tns:FamilyMemberRelationshipType" />
					<element name="startDate" type="date" />
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<simpleType name="FamilyMemberRelationshipType">
		<annotation>
			<documentation>Type of a hierarchical relationship between two
				members of a single family.
			</documentation>
		</annotation>
		<restriction base="string">
