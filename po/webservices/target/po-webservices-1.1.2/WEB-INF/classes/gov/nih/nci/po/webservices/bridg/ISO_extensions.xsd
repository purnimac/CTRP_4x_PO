<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:ISO="uri:iso.org:21090"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" jaxb:version="2.1"
           targetNamespace="http://iso21090.nci.nih.gov"
           elementFormDefault="qualified">

    <xs:import schemaLocation="ISO_datatypes_Narrative.xsd" namespace="uri:iso.org:21090" />

    <xs:annotation>
        <xs:appinfo>
            <jaxb:schemaBindings>
                <jaxb:package name="gov.nih.nci.iso21090.extensions"/>
            </jaxb:schemaBindings>
        </xs:appinfo>
    </xs:annotation>
    
     <xs:element name="Id">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ISO:II" />
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
    
    <xs:element name="Cd">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ISO:CD" />
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="Bl">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ISO:BL" />
            </xs:complexContent>
        </xs:complexType>
    </xs:element>

    <xs:element name="St">
        <xs:complexType>
            <xs:complexContent>
                <xs:extension base="ISO:ST" />
            </xs:complexContent>
        </xs:complexType>
    </xs:element>
</xs:schema>
